{
    "Types": [
        {
            "Mode1": [
                {
                    "RegexName": "Full Numeric Strings",
                    "Pattern": "^[0-9]+$ or ^\\d+$",
                    "Example": "abc123xyz"
                }
            ]
        },
        {
            "Mode2": [
                {
                    "RegexName": "HTTP Addresses",
                    "Pattern": "^http:\\/\\/\\S+(\\/\\S+)*(\\/)?$",
                    "Example": "http://example.com"

                }
            ]
        },
        {
            "Mode3": [
                {
                    "RegexName": "Email Addresses",
                    "Pattern": "^\\w+([.-]?\\w+)*@\\w+([.-]?\\w+)*(\\.\\w{2,3})+$",
                    "Example": "test@example.com"
                }
            ]
        }
    ]  
}
